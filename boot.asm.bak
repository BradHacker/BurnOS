[bits 16]
[org 0x7c00]

init:
  mov ah, 0
  mov al, 0x03
  int 0x10

  mov ax, 0x0700
  mov bh, 0x07 ; white characters on black background
  mov cx, 0x0000 ; put cursor at 0, 0
  mov dx, 0x184f ; row = 24, col = 79
  int 0x10

  mov si, msg
  mov ah, 0x0e
  print_char:
    lodsb ; load si into al and increment si
    cmp al, 0
    je done
    int 0x10
    jmp print_char
  done:
    hlt

msg: db "  _    _      _ _        __          __        _     _ ", 13, 10, " | |  | |    | | |       \ \        / /       | |   | |", 13, 10, " | |__| | ___| | | ___    \ \  /\  / /__  _ __| | __| |", 13, 10, " |  __  |/ _ \ | |/ _ \    \ \/  \/ / _ \| '__| |/ _` |", 13, 10, " | |  | |  __/ | | (_) |    \  /\  / (_) | |  | | (_| |", 13, 10, " |_|  |_|\___|_|_|\\___/     \/  \/ \___/|_|  |_|\__,_|", 13, 10, 0

times 510-($-$$) db 0
dw 0xaa55
